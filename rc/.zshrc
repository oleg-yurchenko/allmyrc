# for x86_64 brew
export PATH=$HOME/bin:/usr/local/bin:$PATH
# for running x86_64 applications
alias rosetta2='arch -x86_64'
alias ibrew='rosetta2 /usr/local/homebrew/bin/brew'

export PATH=/usr/local/lib/omnetpp-5.6.2/bin:$PATH
export PATH=/usr/local/lib/omnetpp-5.6.2/tools/macosx/bin:$PATH
export QT_PLUGIN_PATH=/usr/local/lib/omnetpp-5.6.2/tools/macosx/plugins
export PATH=/opt/homebrew/opt/python@3.10/libexec/bin:$PATH
